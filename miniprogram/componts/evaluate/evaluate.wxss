.container {
    margin-top: 20rpx;
    overflow: hidden;
}

.container .button {
    margin-right: 75rpx;
    margin-bottom: 100rpx;
    width: 200rpx;
    height: 80rpx;
    border-radius: 75rpx;
    background-color: greenyellow;
}
.evaluate_box {
    margin-top: 40rpx;
    display: flex;
    flex-direction: column;
    width: 80%;
    border-radius: 10rpx;
    padding: 20rpx;
    text-align: center;
}
.footer {
    display: flex;
    margin-top: -10rpx;
    margin-left: 320rpx;
    margin-bottom: 30rpx;
    margin-top: 0rpx;
    margin-right: -120rpx;
}
._error{
    border-radius: 25rpx;
    height: 100rpx;
    width: 30rpx;
    margin-top: -80rpx;
    margin-right: 0rpx;
    color: #000;
    background-color: red;
    border-right: 5rpx solid #D2D3D5;
    flex: 1;
    line-height: 100rpx;
    text-align: center;
    font-size: 32rpx;
}
.submitstar {
    border-radius: 25rpx;
    height: 100rpx;
    width: 30rpx;
    margin-top: -80rpx;
    margin-left: 10rpx;
    margin-right: 10rpx;
    background-color: green;
    color: #000;
    flex: 1;
    line-height: 100rpx;
    text-align: center;
    font-size: 32rpx;
}
/*星星评价的每个图片 */
.container .evaluate_box .star-image {
    margin-top: -200rpx;
    margin-left: 20rpx;
    overflow-y: hidden;
    position:absolute;
    width: 80rpx;
    height: 80rpx;
}