<search></search>
<!-- 左列表分类，右列表显示 -->
<view class="flexlist">
	<view class="leftlist">
		<view
		 class="leftlistitem"
		 wx:for="{{list}}"
		 bind:tap="clickTab"
		 data-selectchara="{{item}}"
		>
			{{item}}
		</view>
	</view>
	<view>这是分类后的显示列表</view>
	<view class="rightlist" wx:for="{{catedoc}}">
		<view bind:tap="preview" data-id="{{item.fileid}}">{{item.filename}}</view>
		<evaluate
		 id='evaluate'
		 bind:error="_error"
		 bind:success="_success"
		 paramtoev="{{item._id}}"
		></evaluate>
		<view>此文案得分:"{{item.count}}"</view>
	</view>
</view>

